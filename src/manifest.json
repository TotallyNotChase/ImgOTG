{
  "name": "ImgOTG",
  "version": "1.0",
  "description": "An extension to edit images on a tab on the fly!",
  "permissions": [
      "activeTab"
    ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
